<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bjtu.deliverysystem.model">
  <class name="com.bjtu.deliverysystem.model.UserProfile" table="tb_userprofile">
    <id column="id" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="address" name="address" type="string"/>
    <property column="truename" name="truename" type="string"/>
    <property column="telephone" name="telephone" type="string"/>
    <property column="province" name="province" type="string"/>
    <property column="city" name="city" type="string"/>
  </class>
</hibernate-mapping>
